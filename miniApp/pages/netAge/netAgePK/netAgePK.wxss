.jinduBlock {
  box-sizing: border-box;
  width: 640rpx;
  margin: 6rpx auto 0;
  height: 45rpx;
  background: none;
  border-radius: 30rpx;
  position: relative;
  border: 4rpx #d15033 solid;
  background: #fef6d9;
  overflow: hidden;
}
.jinduBlock::after{
  border: 4rpx #d15033 solid;
}
.jindu_background{
  width: 20%;
  height: 100%;
  display: inline-block;
  background-size: auto 100%;
  background-position: right center;
  border-radius: 60rpx;
  position: relative;
}
.jindu_background::after{
  content: '';
  width: 25rpx;
  height: 25rpx;
  background: #fef6d9;
  display: block;
  position: absolute;
  right: -10rpx;
  top: -10rpx;
  transform: rotate(45deg)
}
.jindu_background::before{
  content: '';
  width: 25rpx;
  height: 25rpx;
  background: #fef6d9;
  display: block;
  position: absolute;
  right: -10rpx;
  bottom: -10rpx;
  transform: rotate(-45deg)
}
.jinduBlock image{
  height: 100%;
  display: inline-block;
}
